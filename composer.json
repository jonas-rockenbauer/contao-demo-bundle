{
    "name": "jrockenbauer/contao-demo-bundle",
    "type": "contao-bundle",
    "description": "Demo for Contao 4 bundles",
    "homepage": "https://contao.org",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Jonas Rockenbauer",
            "homepage": "https://github.com/jrockenbauer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "4.9.*",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Jrockenbauer\\ContaoDemoBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Jrockenbauer\\ContaoDemoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jrockenbauer\\ContaoDemoBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/jonas-rockenbauer/contao-demo-bundle/issues",
        "source": "https://github.com/jonas-rockenbauer/contao-demo-bundle"
    }
}
